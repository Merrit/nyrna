{
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/.hg/store/**": true,
        "**/packaging/linux/appimage/AppDir**": true,
        "**/packaging/linux/appimage/appimage-builder-cache**": true,
    },
    "dart.flutterTestAdditionalArgs": [
        "--coverage"
    ],
    "git.ignoredRepositories": [
        "packaging/linux/flatpak/shared-modules"
    ],
    "git.detectSubmodules": false,
    "dart.lineLength": 90,
    "[dart]": {
        "editor.rulers": [
            90
        ]
    }
}
